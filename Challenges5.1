html {
  background-color: #6bbcbe;
}
body {
  height: 80vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
@keyframes circlemove {
  0% {
    transform: rotateZ(0);
  }
  100% {
    transform: rotateZ(180deg);
  }
}
@keyframes boxmove {
  0%,
  50%,
  100% {
    transform: scaleY(1);
  }

  20% {
    transform: scaleY(2);
  }
}
#rotation {
  display: flex;
  justify-content: center;
  margin-bottom: 80px;
  animation: circlemove 1s ease-in-out infinite;
}
.circle {
  background-color: white;
  width: 25px;
  height: 25px;
  border-radius: 50%;
}
.circle:first-child {
  margin-right: 50px;
}

.circle:nth-child(3) {
  margin-left: 50px;
}
#mainbox {
  display: flex;
  justify-content: center;
  margin-top: 140px;
}
.box {
  background-color: white;
  width: 20px;
  height: 80px;
  animation: boxmove 1s ease-in-out infinite;
}
.box:first-child {
  margin-right: 10px;
  animation-delay: ;
}
.box:nth-child(2) {
  animation-delay: -0.9s;
}
.box:nth-child(3) {
  margin: 0px 10px;
  animation-delay: -0.8s;
}
.box:nth-child(4) {
  animation-delay: -0.7s;
}
.box:nth-child(5) {
  margin-left: 10px;
  animation-delay: -0.6s;
}

